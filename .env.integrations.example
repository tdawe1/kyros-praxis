# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Context7 MCP
CONTEXT7_NODE_IMAGE=node:20-alpine
CONTEXT7_API_TOKEN=
CONTEXT7_DEFAULT_MINIMUM_TOKENS=256

# GitHub MCP
GITHUB_MCP_IMAGE=ghcr.io/github/github-mcp-server
GITHUB_PERSONAL_ACCESS_TOKEN=
GITHUB_TOOLSETS=repos,issues,pull_requests,code_security
GITHUB_READ_ONLY=

# Filesystem MCP
FS_NODE_IMAGE=node:20-alpine
FS_PATH=.
FS_MODE=ro
FS_ALLOW_GLOBS=**/*

# Memory MCP
MEMORY_NODE_IMAGE=node:20-alpine
MEMORY_COLLECTION=kyros_agents

# Browserless
PUPPETEER_MCP_IMAGE=node:20-alpine
BROWSERLESS_TOKEN=

# Playwright
PLAYWRIGHT_IMAGE=mcr.microsoft.com/playwright:v1.45.0-jammy

# Notion MCP
NOTION_MCP_IMAGE=node:20-alpine
OPENAPI_MCP_HEADERS={"Authorization":"Bearer ${NOTION_TOKEN}","Notion-Version":"2022-06-28"}
NOTION_TOKEN=

# Sequential Thinking MCP
SEQ_MCP_IMAGE=node:20-alpine

# Composer Trade MCP
UV_IMAGE=ghcr.io/astral-sh/uv:latest
COMPOSER_API_KEY=
COMPOSER_SECRET_KEY=

# ExaSearch MCP
EXASEARCH_MCP_IMAGE=node:20-alpine
EXA_API_KEY=

# Kyros MCP server
KYROS_MCP_IMAGE=ghcr.io/kyrosai/mcp-kyros:latest
KYROS_API_BASE=http://kyros_orchestrator:8000
KYROS_REGISTRY_URL=http://kyros_registry:9000
KYROS_DAEMON_URL=ws://kyros_daemon:8080/ws
KYROS_MCP_API_KEY=
KYROS_MCP_TOOLS=runs,registry,memory,terminal

# ExaSearch MCP
EXASEARCH_MCP_IMAGE=node:20-alpine
EXA_API_KEY=
