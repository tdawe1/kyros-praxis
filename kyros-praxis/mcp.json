{
  "mcpServers": {
    "zen": {
      "command": "/home/thomas/zen-mcp-server/.zen_venv/bin/python",
      "args": [
        "/home/thomas/zen-mcp-server/server.py"
      ],
      "env": {
        "GEMINI_API_KEY": "",
        "OPENAI_API_KEY": "",
        "ANTHROPIC_API_KEY": "",
        "OPENROUTER_API_KEY": "",
        "DISABLED_TOOLS": "",
        "DEFAULT_MODEL": "auto",
        "WORKSPACE_ROOT": "/home/thomas/kyros-praxis/kyros-praxis",
        "REDIS_URL": "redis://localhost:6379",
        "QDRANT_URL": "https://efc4845b-720a-442e-981d-dc84bf7c7eb0.eu-west-2-0.aws.cloud.qdrant.io:6333",
        "QDRANT_API_KEY": "${QDRANT_API_KEY}",
        "QDRANT_COLLECTION": "kyros",
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "TAVILY_API_KEY": "${TAVILY_API_KEY}",
        "EXA_API_KEY": "${EXA_API_KEY}",
        "GDRIVE_CREDENTIALS_PATH": "/home/thomas/Documents/Keys/gdrive-server-credentials.json",
        "NOTION_API_KEY": "${NOTION_API_KEY}",
        "KYROS_CONFIG": "${KYROS_CONFIG}",
        "COLLAB_CONFIG": "${COLLAB_CONFIG}",
        "FIREPROOF_URL": "${FIREPROOF_URL}",
        "LOG_LEVEL": "INFO",
        "CONVERSATION_TIMEOUT_HOURS": "3",
        "MAX_CONVERSATION_TURNS": "20"
      }
    },
    "playwright": {
      "command": "bash",
      "args": ["scripts/mcp-run.sh", "mcp_playwright"]
    },
    "kyros": {
      "command": "python",
      "args": [
        "integrations/kyros-mcp/main.py"
      ],
      "env": {
        "KYROS_CONFIG": "${KYROS_CONFIG}"
      },
      "disabled": false,
      "alwaysAllow": []
    },
    "kyros-collab": {
      "command": "python",
      "args": [
        "integrations/kyros-collab-mcp/main.py"
      ],
      "env": {
        "COLLAB_CONFIG": "${COLLAB_CONFIG}"
      },
      "disabled": true,
      "alwaysAllow": []
    },
    "fireproof": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "fireproof-mcp"
      ],
      "env": {
        "FIREPROOF_URL": "${FIREPROOF_URL}"
      },
      "disabled": true,
      "alwaysAllow": []
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "~/"
      ],
      "alwaysAllow": [
        "read_text_file"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "alwaysAllow": [
        "read_graph"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "-e",
        "GITHUB_TOOLSETS",
        "-e",
        "GITHUB_READ_ONLY",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}",
        "GITHUB_TOOLSETS": "",
        "GITHUB_READ_ONLY": ""
      }
    },
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "${EXA_API_KEY}"
      }
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--browser=",
        "--headless=",
        "--viewport-size="
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "/home/thomas/kyros-praxis/kyros-praxis"
      ]
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.3"
      ],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "codegen": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/codegen-sh/codegen-sdk.git#egg=codegen-mcp-server&subdirectory=codegen-examples/examples/codegen-mcp-server",
        "codegen-mcp-server"
      ]
    },
    "notion": {
      "command": "npx",
      "args": [
        "-y",
        "@notionhq/notion-mcp-server"
      ],
      "env": {
        "OPENAPI_MCP_HEADERS": "${OPENAPI_MCP_HEADERS}"
      }
    },
    "redis": {
      "command": "uvx",
      "args": [
        "mcp-server-redis",
        "--url",
        "redis://localhost:6379"
      ],
      "disabled": false,
      "alwaysAllow": []
    },
    "qdrant": {
      "command": "uvx",
      "args": [
        "mcp-server-qdrant"
      ],
      "env": {
        "QDRANT_URL": "https://efc4845b-720a-442e-981d-dc84bf7c7eb0.eu-west-2-0.aws.cloud.qdrant.io:6333",
        "QDRANT_API_KEY": "${QDRANT_API_KEY}",
        "COLLECTION_NAME": "kyros",
        "EMBEDDING_MODEL": ""
      }
    },
    "gdrive": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-gdrive"
      ],
      "env": {
        "GDRIVE_CREDENTIALS_PATH": "/home/thomas/Documents/Keys/gdrive-server-credentials.json"
      }
    }
  }
}
