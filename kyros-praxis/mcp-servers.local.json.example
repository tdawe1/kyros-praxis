{
  "mcpServers": {
    "playwright": {
      "command": "bash",
      "args": ["kyros-praxis/scripts/mcp-run.sh", "mcp_playwright"]
    },

    "zen": {
      "command": "bash",
      "args": ["kyros-praxis/scripts/mcp-run.sh", "mcp_zen"]
    },

    "kyros_collab": {
      "command": "bash",
      "args": ["kyros-praxis/scripts/mcp-run.sh", "mcp_kyros_collab"]
    },

    "github_mcp": {
      "command": "bash",
      "args": ["kyros-praxis/scripts/mcp-run.sh", "mcp_github"]
    },

    "vercel": {
      "command": "bash",
